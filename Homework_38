class Group:
    def __init__(self, name, quantity, student):
        self.name = name
        self.quantity = quantity
        self.student = student

    @classmethod
    def generated_group(cls, quantity, name, age, mark):
        students = []
        for i in range(quantity):
            students.append(Student(name, age, mark))
        return cls


class Student:
    def __init__(self, name, age, mark):
        self.name = name
        self.age = age
        self.mark = mark

    @property
    def name(self):
        return self.name

    @name.setter
    def name(self, name):
        self.name = name

    @property
    def age(self):
        return self.age

    @age.setter
    def age(self, age):
        self.age = age

    @property
    def mark(self):
        return self.mark

    @mark.setter
    def mark(self, mark):
        self.mark = mark

    @staticmethod
    def bologna(mark):
        if mark >= 90:
            return 'A'
        elif mark >= 85:
            return 'B'
        elif mark >= 75:
            return 'C'
        elif mark >= 65:
            return 'D'
        elif mark >= 60:
            return 'E'
        else:
            return 'Fx'
